{
  "name": "stimulus-debug",
  "version": "0.0.1",
  "description": "A small Stimulus controller to log events to help with debugging",
  "main": "dist/debug_controller.umd.js",
  "module": "dist/debug_controller.js",
  "amdName": "StimulusDebug",
  "repository": "https://github.com/bullet-train-co/stimulus-debug.git",
  "author": "@pascallaliberte",
  "license": "MIT",
  "external": "stimulus",
  "private": false,
  "scripts": {
    "prebuild": "$npm_execpath run clean",
    "build": "$npm_execpath run build:module && $npm_execpath run build:umd",
    "build:module": "mkdir -p dist/ && cp -R src/debug_controller.js dist/",
    "build:umd": "microbundle src/debug_controller.js -f umd --globals stimulus=Stimulus",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "copy": "^0.3.2",
    "microbundle": "^0.13.0",
    "stimulus": ">=1.1.1 <3"
  },
  "peerDependencies": {
    "stimulus": ">=1.1.1 <3"
  }
}
